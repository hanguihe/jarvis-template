{
  "name": "jarvis",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^6.4.18",
    "@reduxjs/toolkit": "^1.4.0",
    "ahooks": "^2.5.0",
    "antd": "^4.6.1",
    "moment": "^2.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "umi": "^3.2.18"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-react": "^1.6.4",
    "@umijs/test": "^3.2.18",
    "eslint": "^7.7.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.1.1",
    "yorkie": "^2.0.0"
  }
}
